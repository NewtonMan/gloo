{
  "index": "https://github.com/solo-io/glooctl-plugin-index",
  "bucket": {
    "name": "gloo-ee",
    "base_path": "glooctl-plugins"
  },
  "platforms": [
    {
      "os": "darwin",
      "arch": "amd64"
    },
    {
      "os": "linux",
      "arch": "amd64"
    }
  ],
  "plugins": [
    {
      "name": "plugin",
      "description": "The plugin management system for glooctl",
      "build_target": "projects/glooctl-plugins/manager/cmd/plugin/main.go"
    },
    {
      "name": "wasm",
      "description": "Utility to manage Gloo WASM filters",
      "build_target": "projects/glooctl-plugins/wasm/cmd/main.go"
    },
    {
      "name": "fed",
      "description": "Utility to manage Gloo Federation",
      "build_target": "projects/glooctl-plugins/fed/cmd/main.go"
    }
  ]
}
