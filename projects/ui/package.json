  "scripts": {
    "start": "BROWSER=none react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "lint": "eslint \"./src/**/*.tsx\"",
    "fix": "eslint --fix \"./src/**/*.tsx\" && prettier --config ./.prettierrc.yaml --write \"./src/**/*.{ts,tsx}\"",
    "eject": "react-scripts eject",
    "e2e": "jest --verbose --config ./e2e/jest.config.ts ./e2e",
    "screenshots-to-gif": "ffmpeg -y -framerate 1 -pattern_type glob -i './e2e/tests/screenshots/*.jpeg' -r 15 ./e2e/tests/screenshots/out.gif",
    "storybook": "start-storybook -p 6006 -s public",
    "test-storybook:ci": "concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"NODE_ENV=test yarn build-storybook --quiet && npx http-server storybook-static --port 6006 --silent\" \"wait-on tcp:6006 && yarn test-storybook --maxWorkers=3\" ",
    "build-storybook": "build-storybook -s public",
    "test-storybook": "test-storybook --maxWorkers=3",
    "build:tailwind": "npx tailwindcss -i ./src/Styles/precompile.css -o ./src/Styles/styles.css",
    "watch:tailwind": "yarn build:tailwind --watch"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not ie <= 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "ace-builds": "<rootDir>/node_modules/ace-builds"
    }
  },
  "devDependencies": {
    "@alenon/grpc-mock-server": "^3.0.7",
